package com.kuaileren.domain;

import java.util.ArrayList;
import java.util.List;

public class CountInfoDO {

	private KuaiLeDO yesterday;
	
	private List<KuaiLeDO> lastday = new ArrayList<KuaiLeDO>();
	
	private List<ArticleDO> lastArticle = new ArrayList<ArticleDO>();

	
	public KuaiLeDO getYesterday() {
		return yesterday;
	}

	public void setYesterday(KuaiLeDO yesterday) {
		this.yesterday = yesterday;
	}

	public List<KuaiLeDO> getLastday() {
		return lastday;
	}

	public void setLastday(List<KuaiLeDO> lastday) {
		this.lastday = lastday;
	}

	public List<ArticleDO> getLastArticle() {
		return lastArticle;
	}

	public void setLastArticle(List<ArticleDO> lastArticle) {
		this.lastArticle = lastArticle;
	}
	
}
